    "antd": "^4.20.0",
    "dayjs": "^1.11.1",
    "dexie": "^3.2.1",
    "dexie-react-hooks": "^1.1.1",
    "pixi.js": "^6.1.3",
    "qrcode.react": "^3.0.1",
    "react": "17.x",
    "react-color": "^2.19.3",
    "react-dom": "17.x",
    "swiper": "^8.0.7",
    "umi": "^3.5.20",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.4",
    "@umijs/preset-react": "1.x",
